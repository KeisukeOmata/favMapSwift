    "next-page-tester": "^0.24.1",
    "next-unused": "^0.0.3",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^2.2.1",
    "stylelint": "^13.9.0",
    "stylelint-config-standard": "^20.0.0",
    "ts-jest": "^26.4.4",
    "typescript": "^4.2.3"
  }
