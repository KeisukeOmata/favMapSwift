{
  "name": "next_e-commerce",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "dev": "next dev",
    "build": "next build && next-sitemap --config sitemap.config.js",
    "start": "next start",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx --ignore-path .gitignore .",
    "test": "jest src/__tests__/.*/*.test.tsx?$",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "cypress:run:chrome": "cypress run --browser chrome",
    "cypress:run:firefox": "cypress run --browser firefox",
    "storybook": "start-storybook -p 6006 -c .storybook",
    "find:unused": "next-unused",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "yarn lint --fix",
      "yarn test"
    ]
  },
  "next-unused": {
    "debug": true,
    "include": [
      "src/components",
      "src/lib",
      "src/styles"
    ],
    "exclude": [],
    "entrypoints": [
      "src/pages"
    ]
  },
  "dependencies": {
    "autoprefixer": "^10.2.6",
    "classnames": "^2.3.1",
    "dayjs": "^1.10.5",
    "firebase": "^8.6.3",
    "keen-slider": "^5.4.1",
    "next": "^11.0.0",
    "next-pwa": "^5.2.21",
    "next-seo": "^4.24.0",
    "next-themes": "^0.0.14",
    "postcss": "^8.3.0",
    "pwacompat": "^2.0.17",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-hook-form": "^7.7.1",
    "react-toastify": "^7.0.4",
    "recoil": "^0.3.1",
    "shopify-buy": "^2.11.0",
    "tailwindcss": "^2.2.0"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^10.2.3",
    "@storybook/react": "^6.2.9",
    "@testing-library/dom": "^7.31.0",
    "@testing-library/jest-dom": "^5.12.0",
    "@testing-library/react": "^11.2.7",
    "@testing-library/react-hooks": "^7.0.0",
    "@testing-library/user-event": "^13.1.9",
    "@types/jest": "^26.0.23",
    "@types/node": "^15.6.1",
    "@types/react": "^17.0.8",
    "@types/shopify-buy": "^2.10.5",
    "@typescript-eslint/eslint-plugin": "^4.25.0",
    "@typescript-eslint/parser": "^4.25.0",
    "cypress": "^7.4.0",
    "cypress-localstorage-commands": "^1.4.5",
    "eslint": "^7.27.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-tailwindcss": "^1.8.5",
    "husky": "^6.0.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.0.3",
    "jest-css-modules": "^2.1.0",
    "lint-staged": "^11.0.0",
    "msw": "^0.29.0",
    "next-page-tester": "^0.25.0",
    "next-sitemap": "^1.6.97",
    "next-unused": "^0.0.6",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^2.3.0",
    "ts-jest": "^27.0.1",
    "typescript": "^4.3.2"
  }
}
