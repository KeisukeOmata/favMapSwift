    "next-themes": "^0.0.10",
    "postcss": "^8.2.10",
    "pwacompat": "^2.0.17",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-toastify": "^7.0.3",
    "recoil": "^0.1.2",
    "shopify-buy": "^2.11.0",
    "tailwindcss": "^2.1.1"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^10.2.0",
